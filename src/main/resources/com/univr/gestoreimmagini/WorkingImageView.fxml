<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/null" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.univr.gestoreimmagini.WorkingImageController">
   <children>
      <Button fx:id="button" layoutX="108.0" layoutY="55.0" mnemonicParsing="false" onAction="#switchToImageEditorView" text="Go back" />
      <AnchorPane layoutX="58.0" layoutY="251.0" prefHeight="134.0" prefWidth="483.0">
         <children>
            <ImageView fx:id="mainImage" fitHeight="115.0" fitWidth="143.0" layoutX="170.0" layoutY="9.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="leftImage" fitHeight="95.0" fitWidth="113.0" layoutX="57.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true" />
            <ImageView fx:id="rightImage" fitHeight="95.0" fitWidth="113.0" layoutX="313.0" layoutY="20.0" pickOnBounds="true" preserveRatio="true" />
            <Button layoutX="438.0" layoutY="54.0" mnemonicParsing="false" onAction="#scrollRight" prefHeight="30.0" prefWidth="30.0" text="+" />
            <Button layoutX="14.0" layoutY="54.0" mnemonicParsing="false" onAction="#scrollLeft" prefHeight="30.0" prefWidth="30.0" text="-" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
